{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "bookings": {
      "$bookingId": {
        "data": {
          // Let anyone knowing the bookingId (a uuid4) read/write access to the `data` array.
          ".read": true,
          ".write": true
        }
      }
    }
  }
}