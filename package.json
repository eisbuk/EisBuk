{
  "private": true,
  "name": "eisbuk",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "lint-staged": {
