  "scripts": {
    "build": "echo \"Skipping @eisbuk/client build as part of rush's bulk 'build' script. To build the app for production run: 'rushx build:prod'\"",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "build:prod": "BUILD_ENV=production vite build",
    "build:test": "nyc instrument --compact=false src instrumented && cp -R -u -p src/* instrumented/ && USE_INSTRUMENTED=true BUILD_ENV=test vite build",
    "start": "vite",
    "start:eisbuk": "REACT_APP_EISBUK_SITE=eisbuk.web.app BUILD_ENV=production vite",
    "start:igorice": "REACT_APP_EISBUK_SITE=igorice.web.app BUILD_ENV=production vite",
    "start:staging": "REACT_APP_EISBUK_SITE=eisbuk.web.app BUILD_ENV=production DEPLOY_STAGE=staging vite",
    "test:emulators": "bash -c '(trap \"kill 0\" SIGINT; ../functions/build_scripts/watch.js & firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --watch --runInBand --detectOpenHandles src\")'",
    "test:emulators:ci": "bash -c '(trap \"kill 0\" SIGINT; ../functions/build_scripts/watch.js & firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --ci --reporters=default --reporters=jest-junit --collect-coverage --forceExit --detectOpenHandles --runInBand\")'",
    "test:quicktest": "echo Running tests with no emulators support && jest --watch src",
    "test": "echo 'Running all tests; using emulators.' && rushx test:emulators",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
