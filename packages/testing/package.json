      "default": "./dist/envData/index.js"
    },
    "./migrations": {
      "import": "./dist/migrations/index.es.js",
      "require": "./dist/migrations/index.js",
      "default": "./dist/migrations/index.js"
    },
    "./testIds": {
      "import": "./dist/testIds/index.es.js",
