      "default": "./dist/customers/index.js"
    },
    "./dataTriggers": {
      "import": "./dist/dataTriggers/index.es.js",
      "require": "./dist/dataTriggers/index.js",
      "default": "./dist/dataTriggers/index.js"
    },
    "./date": {
      "import": "./dist/date/index.es.js",
