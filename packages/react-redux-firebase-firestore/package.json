{
  "name": "@eisbuk/react-redux-firebase-firestore",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js",
      "default": "./dist.index.es.js"
    }
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@firebase/app": "0.7.2",
    "@firebase/auth": "^0.19.11",
    "@firebase/firestore": "3.1.0",
    "firebase": "9.1.1",
    "firebase-tools": "^9.16.6",
    "luxon": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test:emulators": "bash -c '(trap \"kill 0\" SIGINT; ../functions/build_scripts/watch.js & firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --watch --runInBand --detectOpenHandles src\")'",
    "test:emulators:ci": "bash -c '(trap \"kill 0\" SIGINT; ../functions/build_scripts/watch.js & firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --ci --reporters=default --reporters=jest-junit --collect-coverage --forceExit --detectOpenHandles --runInBand\")'",
    "test:quicktest": "echo Running tests with no emulators support && jest --watch src",
    "test": "echo 'Running all tests; using emulators.' && rushx test:emulators"
  },
  "devDependencies": {
    "@eisbuk/shared": "workspace:*",
    "@firebase/app": "0.7.2",
    "@firebase/auth": "^0.19.11",
    "@firebase/firestore": "3.1.0",
    "@firebase/rules-unit-testing": "^2.0.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.0.5",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@types/uuid": "^8.3.2",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase": "9.1.1",
    "firebase-tools": "^9.16.6",
    "jest": "^27.5.1",
    "luxon": "^2.0.5",
    "p-retry": "^4.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "~4.6.3",
    "uuid": "^8.3.2",
    "vite": "^2.9.9"
  }
}
