    "@firebase/app-compat": "^0.1.21",
    "@google-cloud/firestore": "^4.13.2",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "lodash": "^4.17.20",
    "luxon": "^2.0.5",
    "p-retry": "^4.6.1",
    "retry": "^0.13.1",
    "ts-node": "^10.5.0",
    "unix-timestamp": "^0.2.0",
    "uuid": "^8.3.2",
    "nodemailer": "~6.7.3",
    "ajv": "~8.11.0",
    "ajv-errors": "~3.0.0",
