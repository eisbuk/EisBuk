{
  "name": "@eisbuk/backup-restore",
  "version": "1.0.0",
  "description": "CLI for firestore backup-restore",
  "private": true,
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js",
      "default": "./dist.index.es.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc-bundle tsconfig-build.json && chmod 755 dist/cli.js && sed -i '1 i #!/usr/bin/env node' dist/cli.js",
    "build-link": "rushx build && pnpm link --global",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "bash -c '(trap \"kill 0\" SIGINT; firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --watch --runInBand --detectOpenHandles src\")'"
  },
  "dependencies": {
    "@eisbuk/shared": "workspace:*",
    "firebase-admin": "^9.4.2",
    "commander": "^8.3.0",
    "configstore": "^5.0.0",
    "env-paths": "^2.2.1",
    "inquirer": "^8.0.0",
    "typescript-bundle": "~1.0.18"
  },
  "devDependencies": {
    "@babel/core": "7.x",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-typescript": "^7.14.5",
    "@google-cloud/firestore": "^4.13.2",
    "@grpc/grpc-js": "^1.6.2",
    "@types/jest": "^27.4.1",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "p-retry": "^4.6.1",
    "jest": "^27.5.1",
    "typescript": "~4.6.3",
    "uuid": "^8.3.2",
    "@types/configstore": "~5.0.1"
  },
  "bin": {
    "firestore-dump": "./dist/cli.js"
  }
}
