{
  "name": "@eisbuk/firestore-process-delivery",
  "version": "1.0.0",
  "description": "A lightweight utility used to process a delivery while using the firestore document as a trigger/report",
  "scripts": {
    "build": "vite build && tsc",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "bash -c '(trap \"kill 0\" SIGINT; firebase -c ../../firebase-testing.json  emulators:exec --project eisbuk -- \" jest --watch --runInBand --detectOpenHandles src\")'",
    "test:verbose": "LOG_LEVEL=verbose rushx test"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js",
      "default": "./dist.index.es.js"
    }
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@firebase/app-compat": "^0.1.21",
    "@google-cloud/firestore": "^4.13.2",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.x",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-typescript": "^7.14.5",
    "@firebase/app-types": "^0.7.0",
    "@grpc/grpc-js": "^1.6.2",
    "@types/jest": "~27.4.1",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unused-imports": "^1.0.1",
    "firebase-functions-test": "^0.2.3",
    "jest": "~27.5.1",
    "mocha": "^8.2.1",
    "typescript": "~4.4.2",
    "vite": "~2.9.5",
    "vite-plugin-environment": "^1.1.0"
  },
  "private": true
}
