{
  "name": "@eisbuk/firestore-process-delivery",
  "version": "1.0.0",
  "description": "A lightweight utility used to process a delivery while using the firestore document as a trigger/report",
  "scripts": {
    "build": "vite build && tsc",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js",
      "default": "./dist.index.es.js"
    }
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@firebase/app-compat": "^0.1.21",
    "@google-cloud/firestore": "^4.13.2",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "p-retry": "^4.6.1",
    "retry": "^0.13.1",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.7.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unused-imports": "^1.0.1",
    "firebase-functions-test": "^0.2.3",
    "mocha": "^8.2.1",
    "typescript": "~4.4.2"
  },
  "private": true
}
