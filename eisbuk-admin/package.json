  "scripts": {
    "emulators:inspect": "bash -c '(trap \"kill 0\" SIGINT; ./functions/build_scripts/watch.js & firebase emulators:start --project eisbuk --inspect-functions 9229)'",
    "emulators:start": "bash -c '(trap \"kill 0\" SIGINT; ./functions/build_scripts/watch.js & firebase emulators:start --project eisbuk)'",
    "build": "node esbuild/bundler.js",
    "build:test": "NODE_ENV=test node esbuild/bundler.js",
    "start": "node esbuild/bundler.js --mode serve",
    "start:eisbuk": "REACT_APP_EISBUK_SITE=eisbuk.web.app NODE_ENV=production node esbuild/bundler.js --mode serve",
    "start:igorice": "REACT_APP_EISBUK_SITE=igorice.web.app NODE_ENV=production node esbuild/bundler.js --mode serve",
    "start:staging": "REACT_APP_EISBUK_SITE=eisbuk.web.app NODE_ENV=production DEPLOY_STAGE=staging node esbuild/bundler.js --mode serve",
    "test:emulators": "bash -c '(trap \"kill 0\" SIGINT; ./functions/build_scripts/watch.js & firebase -c firebase-testing.json  emulators:exec --project eisbuk -- \"yarn jest --watch --runInBand --detectOpenHandles src\")'",
    "test:emulators:ci": "bash -c '(trap \"kill 0\" SIGINT; ./functions/build_scripts/watch.js & firebase -c firebase-testing.json  emulators:exec --project eisbuk -- \"yarn jest --ci --reporters=default --reporters=jest-junit --collect-coverage --forceExit --detectOpenHandles --runInBand\")'",
    "test:quicktest": "echo Running tests with no emulators support && yarn jest --watch src",
    "test": "echo 'Running all tests; using emulators.' && yarn test:emulators",
    "deploy:eisbuk": "firebase deploy --project eisbuk --only hosting:eisbuk",
    "deploy:production": "./deploy-firebase.expect",
    "deploy:preview": "firebase --project eisbuk hosting:channel:deploy",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
